{
  "extends": [
    "next",
    "next/core-web-vitals",
    "prettier",
    "next/typescript",
    "plugin:storybook/recommended",
    "next/core-web-vitals",
    "eslint:recommended"
  ],
  "plugins": ["prettier"],

  "globals": {
    "React": "readonly"
  },
  "overrides": [
    {
      "files": ["*.stories.@(ts|tsx|js|jsx|mjs|cjs)"]
    }
  ],
  "rules": {
    "no-unused-vars": "warn",
    "prettier/prettier": "warn",
    "no-console": "warn"

  }
}